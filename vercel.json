{
    "rewrites": [
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/category/:path*",
            "destination": "/products",
            "permanent": true
        },
        {
            "source": "/brand/:path*",
            "destination": "/brands/:path*",
            "permanent": true
        }
    ]
}